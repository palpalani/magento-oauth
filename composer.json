{
    "name": "jonnyw/magento-oauth",
    "description": "Magento OAuth Test",
	"type": "library",
    "keywords": [
        "magento oauth",
        "palpalani"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "palpalani",
            "email": "palani.p@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
    	"php": "^8.0",
        "lusitanian/oauth": "dev-master"
    },
    "require-dev": {
        "laravel/pint": "^1.2",
        "pestphp/pest": "^2.34"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:palpalani/PHPoAuthLib.git",
            "no-api": true
        }
    ],
    "autoload": {
        "psr-4": {
            "JonnyW\\MagentoOAuth\\": "src"
        },
        "classmap": ["src/"]
    },
    "autoload-dev": {
        "psr-4": {
            "JonnyW\\MagentoOAuth\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "prefer-stable": true
}
